{
  "name": "Z-Wave JS Server DEVELOPMENT",
  "version": "0.0.10",
  "slug": "zwave_js_server",
  "description": "Not for production purposes, use at your own risk. Will grab latest version from Zwave JS server directly from Github master.",
  "url": "https://github.com/zwave-js/zwave-js-server",
  "startup": "system",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "map": ["config:rw"],
  "boot": "auto",
  "options": {
    "server_branch_or_commit": "zwave-js/zwave-js-server#master",
    "driver_branch_or_commit": "zwave-js/node-zwave-js#master",
    "serial_port": "/dev/ttyACM0",
    "storage": {
      "cacheDir": "/config/.zwjs"
    },
    "networkKey": ""
  },
  "schema": {
    "server_branch_or_commit": "str",
    "driver_branch_or_commit": "str",
    "serial_port": "str",
    "storage": {
      "cacheDir": "str"
    },
    "networkKey": "str"
  },
  "ingress": false,
  "ports": {
    "3000/tcp": 3000
  },
  "auto_uart": true
}